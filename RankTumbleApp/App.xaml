<?xml version="1.0" encoding="UTF-8" ?>
<Application
    x:Class="RankTumbleApp.App"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:RankTumbleApp">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />

                <ResourceDictionary Source="Resources/Styles/AppFontFamilies.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <Style x:Key="FontIconMoonUltimate" TargetType="Label">
                <Setter Property="FontFamily" Value="{DynamicResource IcoMoonUltimate}" />
            </Style>
        </ResourceDictionary>

    </Application.Resources>
</Application>
